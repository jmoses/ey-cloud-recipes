check process queued_messages_<%= @app_name %>
  with pidfile /data/<%= @app_name %>/current/log/queued_message_processor.rb.pid
  start program = "/data/<%= @app_name%>/shared/bin/queued_messaging_system.sh <%= @app_name %> start" as uid <%= @user %> and gid <%= @user %>
  stop program = "/data/<%= @app_name%>/shared/bin/queued_messaging_system.sh <%= @app_name %> stop" as uid <%= @user %> and gid <%= @user %>
  group queued_messages_<%= @app_name %>
